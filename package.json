{
  "name": "vscode-jetbrains-keybindings",
  "displayName": "vscode-jetbrains-keybindings",
  "description": "JetBrains IDE Keymap extension for VS Code",
  "version": "0.0.1",
  "publisher": "sudoz",
  "license": "MIT",
  "engines": {
    "vscode": "^1.19.0"
  },
  "categories": [
    "Keymaps"
  ],
  "keywords": [
    "keymap",
    "keybindings",
    "jetbrains"
  ],
  "preview": true,
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/isudox/vscode-jetbrains-keybindings.git"
  },
  "bugs": {
    "url": "https://github.com/isudox/vscode-jetbrains-keybindings/issues",
    "email": "me@isudox.com"
  },
  "contributes": {
    "keybindings": [
      {
        "mac": "ctrl+space",
        "win": "ctrl+space",
        "linux": "ctrl+space",
        "key": "ctrl+space",
        "command": "editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly",
        "jetbrains": "Basic code completion (the name of any class, method or variable)"
      },
      {
        "mac": "cmd+shift+enter",
        "win": "ctrl+shift+enter",
        "linux": "ctrl+shift+enter",
        "key": "ctrl+shift+enter",
        "command": "acceptSelectedSuggestion",
        "when": "editorFocus && suggestWidgetVisible",
        "jetbrains": "Complete statement"
      },
      {
        "mac": "cmd+p",
        "win": "ctrl+p",
        "linux": "ctrl+p",
        "key": "ctrl+p",
        "command": "editor.action.triggerParameterHints",
        "when": "editorHasSignatureHelpProvider && editorTextFocus",
        "jetbrains": "Parameter info (within method call arguments)"
      },
      {
        "mac": "ctrl+j",
        "win": "ctrl+q",
        "linux": "ctrl+q",
        "key": "ctrl+q",
        "command": "editor.action.showHover",
        "when": "editorTextFocus",
        "jetbrains": "Quick documentation"
      },
      {
        "mac": "cmd+f1",
        "win": "ctrl+f1",
        "linux": "ctrl+f1",
        "key": "ctrl+f1",
        "command": "workbench.action.showErrorsWarnings",
        "when": "editorTextFocus",
        "jetbrains": "Error Description"
      },
      {
        "mac": "cmd+n",
        "win": "alt+insert",
        "linux": "alt+insert",
        "key": "alt+insert",
        "command": "workbench.action.files.newUntitledFile",
        "when": "!editorTextFocus",
        "jetbrains": "Generate code... (Getters, Setters, Constructors, hashCode/equals, toString)"
      },
      {
        "mac": "cmd+/",
        "key": "ctrl+/",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly",
        "jetbrains": "Comment with Line Comment"
      },
      {
        "key": "ctrl+numpad_divide",
        "mac": "cmd+numpad_divide",
        "command": "editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly",
        "jetbrains": "Comment/uncomment with line comment"
      },
      {
        "key": "ctrl+alt+/",
        "mac": "cmd+alt+/",
        "command": "editor.action.blockComment",
        "when": "editorTextFocus && !editorReadonly",
        "jetbrains": "Comment/uncomment with block comment"
      },
      {
        "key": "ctrl+alt+numpad_divide",
        "mac": "cmd+alt+numpad_divide",
        "command": "editor.action.blockComment",
