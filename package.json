{
    "name": "vscode-jetbrains-keybindings",
    "displayName": "JetBrains IDE Keymap",
    "description": "JetBrains IDE Keymap extension for VS Code.",
    "version": "0.1.0",
    "publisher": "isudox",
    "license": "MIT",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "keymap",
        "keybindings",
        "jetbrains",
        "intellij idea",
        "pycharm",
        "webstorm"
    ],
    "preview": true,
    "icon": "jetbrains_keyboard.png",
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "repository": {
        "type": "git",
        "url": "https://github.com/isudox/vscode-jetbrains-keybindings.git"
    },
    "bugs": {
        "url": "https://github.com/isudox/vscode-jetbrains-keybindings/issues",
        "email": "me@isudox.com"
    },
    "contributes": {
        "keybindings": [
            {
                "key": "shift+cmd+p",
                "mac": "shift+cmd+a",
                "command": "workbench.action.showCommands",
                "jetbrains": "Find Action"
            },
            {
                "key": "ctrl+shift+n",
                "mac": "cmd+shift+o",
                "command": "workbench.action.quickOpen",
                "jetbrains": "Go to file"
            },
            {
                "key": "ctrl+f4",
                "mac": "cmd+w",
                "command": "workbench.action.closeActiveEditor",
                "jetbrains": "Close active editor tab"
            },
            {
                "key": "ctrl+alt+s",
                "mac": "cmd+,",
                "command": "workbench.action.openGlobalSettings",
                "jetbrains": "Open Settings dialog"
            },
            {
                "key": "ctrl+x",
                "mac": "cmd+x",
                "command": "editor.action.clipboardCutAction",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Cut current line to clipboard"
            },
            {
                "key": "ctrl+c",
                "mac": "cmd+c",
                "command": "editor.action.clipboardCopyAction",
                "when": "editorTextFocus",
                "jetbrains": "Copy current line to clipboard"
            },
            {
                "key": "shift+ctrl+up",
                "mac": "shift+alt+up",
                "command": "editor.action.moveLinesUpAction",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Move Line Up"
            },
            {
                "key": "shift+ctrl+down",
                "mac": "shift+alt+down",
                "command": "editor.action.moveLinesDownAction",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Move Line Down"
            },
            {
                "key": "ctrl+y",
                "mac": "cmd+backspace",
                "command": "editor.action.deleteLines",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Delete Line"
            },
            {
                "key": "shift+enter",
                "mac": "shift+enter",
                "command": "editor.action.insertLineAfter",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Start New Line"
            },
            {
                "key": "shift+cmd+enter",
                "mac": "cmd+alt+enter",
                "command": "editor.action.insertLineBefore",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Start New Line Before current"
            },
            {
                "key": "ctrl+-",
                "mac": "cmd+-",
                "command": "editor.fold",
                "when": "editorFocus",
                "jetbrains": "Collapse"
            },
            {
                "key": "ctrl+=",
                "mac": "cmd+=",
                "command": "editor.unfold",
                "when": "editorFocus",
                "jetbrains": "Expand"
            },
            {
                "key": "cmd+k cmd+0",
                "command": "editor.foldAll",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+-",
                "mac": "cmd+shift+-",
                "command": "editor.foldAll",
                "when": "editorFocus",
                "jetbrains": "Collapse All"
            },
            {
                "key": "ctrl+shift+=",
                "mac": "cmd+shift+=",
                "command": "editor.unfoldAll",
                "when": "editorFocus",
                "jetbrains": "Expand All"
            },
            {
                "key": "shift+alt+a",
                "command": "editor.action.blockComment",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+alt+/",
                "mac": "cmd+alt+/",
                "command": "editor.action.blockComment",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Comment With Block Comment"
            },
            {
                "key": "ctrl+shift+alt+j",
                "mac": "ctrl+cmd+g",
                "command": "editor.action.selectHighlights",
                "when": "editorFocus",
                "jetbrains": "Select All Occurrences"
            },
            {
                "key": "ctrl+w",
                "mac": "alt+up",
                "command": "editor.action.smartSelect.grow",
                "when": "editorTextFocus",
                "jetbrains": "Extend Selection"
            },
            {
                "key": "ctrl+shift+w",
                "mac": "alt+down",
                "command": "editor.action.smartSelect.shrink",
                "when": "editorTextFocus",
                "jetbrains": "Shrink Selection"
            },
            {
                "key": "ctrl+f",
                "mac": "cmd+f",
                "command": "actions.find",
                "jetbrains": "Find"
            },
            {
                "key": "ctrl+r",
                "mac": "cmd+r",
                "command": "editor.action.startFindReplaceAction",
                "jetbrains": "Replace"
            },
            {
                "key": "f3",
                "mac": "cmd+g",
                "command": "editor.action.nextMatchFindAction",
                "when": "editorFocus",
                "jetbrains": "Find Next"
            },
            {
                "key": "shift+f3",
                "mac": "cmd+shift+g",
                "command": "editor.action.previousMatchFindAction",
                "when": "editorFocus",
                "jetbrains": "Find Previous"
            },
            {
                "key": "alt+j",
                "mac": "ctrl+g",
                "command": "editor.action.selectAllMatches",
                "when": "editorFocus && findWidgetVisible",
                "jetbrains": "Select Next Occurrences"
            },
            {
                "key": "alt+j",
                "mac": "ctrl+g",
                "command": "editor.action.addSelectionToNextFindMatch",
                "when": "editorFocus",
                "jetbrains": "Add Selection for Next Occurrence"
            },
            {
                "key": "ctrl+space",
                "command": "editor.action.triggerSuggest",
                "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
            },
            {
                "mac": "ctrl+space",
                "key": "ctrl+space",
                "command": "editor.action.triggerSuggest",
                "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly",
                "jetbrains": "Basic Code Completion"
            },
            {
                "mac": "cmd+p",
                "key": "ctrl+p",
                "command": "editor.action.triggerParameterHints",
                "when": "editorHasSignatureHelpProvider && editorTextFocus",
                "jetbrains": "Parameter info (within method call arguments)"
            },
            {
                "key": "ctrl+alt+l",
                "mac": "cmd+alt+l",
                "command": "editor.action.formatDocument",
                "when": "editorTextFocus && !editorReadonly",
                "jetbrains": "Reformat Code"
            },
            {
                "key": "ctrl+alt+l",
                "mac": "cmd+alt+l",
                "command": "editor.action.formatSelection",
                "when": "editorHasSelection && editorTextFocus && !editorReadonly",
                "jetbrains": "Reformat Selected Code"
            },
            {
                "key": "ctrl+b",
                "win": "ctrl+b",
                "mac": "cmd+b",
                "command": "editor.action.goToDeclaration",
                "when": "editorHasDefinitionProvider && editorTextFocus",
                "jetbrains": "Go to Declaration"
            },
            {
                "key": "ctrl+shift+i",
                "mac": "alt+space",
                "command": "editor.action.previewDeclaration",
                "when": "editorHasDefinitionProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor",
                "jetbrains": "Open Quick Definition Lookup"
            },
            {
                "key": "cmd+.",
                "command": "editor.action.quickFix",
                "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
            },
            {
                "key": "alt+enter",
                "mac": "alt+enter",
                "command": "editor.action.quickFix",
                "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly",
                "jetbrains": "Show intention actions and quick-fixes"
            },
            {
                "key": "alt+f7",
                "mac": "alt+f7",
                "command": "editor.action.referenceSearch.trigger",
                "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor",
                "jetbrains": "Find Usages"
            },
            {
                "key": "shift+f6",
                "mac": "shift+f6",
                "command": "editor.action.rename",
                "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly",
                "jetbrains": "Rename Symbol"
            },
            {
                "key": "ctrl+n",
                "mac": "cmd+o",
                "command": "workbench.action.showAllSymbols",
                "jetbrains": "Go to Symbol"
            },
            {
                "key": "ctrl+g",
                "mac": "cmd+l",
                "command": "workbench.action.gotoLine",
                "jetbrains": "Go to Line"
            },
            {
                "key": "ctrl+shift+n",
                "mac": "cmd+shift+o",
                "command": "workbench.action.quickOpen",
                "jetbrains": "Go to File"
            },
            {
                "key": "ctrl+e",
                "mac": "cmd+e",
                "command": "workbench.action.quickOpenNavigateNext",
                "when": "inQuickOpen",
                "jetbrains": "Recent files popup"
            },
            {
                "key": "ctrl+f12",
                "mac": "cmd+f12",
                "command": "workbench.action.gotoSymbol",
                "when": "editorTextFocus",
                "jetbrains": "File Structure Popup"
            },
            {
                "key": "ctrl+alt+shift+n",
                "mac": "cmd+alt+o",
                "command": "workbench.action.gotoSymbol",
                "jetbrains": "Go to Symbol"
            },
            {
                "key": "shift+escape",
                "mac": "shift+escape",
                "command": "workbench.actions.view.problems",
                "when": "problemFocus",
                "jetbrains": "Hide active or last active window (Problems)"
            },
            {
                "key": "ctrl+alt+left",
                "mac": "cmd+alt+left",
                "command": "workbench.action.navigateBack",
                "jetbrains": "Navigate Back"
            },
            {
                "key": "",
                "mac": "cmd+[",
                "command": "workbench.action.navigateBack",
                "jetbrains": "Navigate Back"
            },
            {
                "key": "ctrl+shift+-",
                "command": "workbench.action.navigateForward"
            },
            {
                "key": "ctrl+alt+right",
                "mac": "cmd+alt+right",
                "command": "workbench.action.navigateForward",
                "jetbrains": "Navigate Forward"
            },
            {
                "key": "",
                "mac": "cmd+]",
                "command": "workbench.action.navigateForward",
                "jetbrains": "Navigate Forward"
            },
            {
                "key": "ctrl+f4",
                "mac": "cmd+w",
                "command": "workbench.action.closeActiveEditor",
                "jetbrains": "Close Active Editor Tab"
            },
            {
                "key": "shift+alt+tab",
                "mac": "shift+alt+tab",
                "command": "workbench.action.focusPreviousGroup",
                "when": "editorFocus",
                "jetbrains": "Goto previous splitter"
            },
            {
                "key": "alt+tab",
                "mac": "alt+tab",
                "command": "workbench.action.focusNextGroup",
                "when": "editorFocus",
                "jetbrains": "Goto next splitter"
            },
            {
                "mac": "cmd+n",
                "key": "alt+insert",
                "command": "workbench.action.files.newUntitledFile",
                "jetbrains": "Create New File"
            },
            {
                "key": "ctrl+f8",
                "mac": "cmd+f8",
                "command": "editor.debug.action.toggleBreakpoint",
                "when": "editorTextFocus",
                "jetbrains": "Toggle Breakpoint"
            },
            {
                "key": "f9",
                "mac": "f9",
                "command": "workbench.action.debug.continue",
                "when": "inDebugMode",
                "jetbrains": "Resume Program"
            },
            {
                "key": "f7",
                "mac": "f7",
                "command": "workbench.action.debug.stepInto",
                "when": "inDebugMode",
                "jetbrains": "Step Into"
            },
            {
                "key": "shift+f8",
                "mac": "shift+f8",
                "command": "workbench.action.debug.stepOut",
                "when": "inDebugMode",
                "jetbrains": "Step Out"
            },
            {
                "key": "f8",
                "mac": "f8",
                "command": "workbench.action.debug.stepOver",
                "when": "inDebugMode",
                "jetbrains": "Step Over"
            },
            {
                "key": "cmd+f2",
                "command": "workbench.action.debug.stop",
                "when": "inDebugMode"
            },
            {
                "key": "cmd+k cmd+i",
                "command": "editor.debug.action.showDebugHover",
                "when": "editorTextFocus && inDebugMode"
            },
            {
                "mac": "ctrl+j",
                "key": "ctrl+q",
                "command": "editor.action.showHover",
                "when": "editorTextFocus",
                "jetbrains": "Quick documentation"
            },
            {
                "key": "shift+escape",
                "mac": "shift+escape",
                "command": "workbench.action.terminal.toggleTerminal",
                "intellij": "Hide Active or last Active indow (Terminal)"
            },
            {
                "key": "cmd+c",
                "command": "workbench.action.terminal.copySelection",
                "when": "terminalFocus && terminalTextSelected"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "JetBrains IDE Keymap configuration",
            "properties": {
                "sublimeTextKeymap.promptV3Features": {
                    "type": [
                        "boolean",
                        "null"
                    ],
                    "default": null,
                    "description": "JetBrains IDE Keymap configuration."
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
